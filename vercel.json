{
  "version": 2,
  "buildCommand": "npm run build",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    },
    {
      "source": "/v1/:path*",
      "destination": "/api/index.js"
    },
    {
      "source": "/health",
      "destination": "/api/index.js"
    },
    {
      "source": "/s/:token",
      "destination": "/api/index.js"
    },
    {
      "source": "/legacy",
      "destination": "/api/index.js"
    }
  ]
}
